{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-angle-bracket-type-assertion": false,
    "no-var-requires": false,
    "array-type": false,
    "max-line-length": [
      true,
      140
    ],
    "object-shorthand-properties-first": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "prefer-for-of": false,
    "no-boolean-literal-compare": false
  },
  "rulesDirectory": []
}